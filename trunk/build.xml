<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project basedir="." default="build" name="GW">
<property environment="env"/>

    <property name="GW-Application-Arquigrafia.location" value="GW-Application-Arquigrafia"/>
    <property name="GW.location" value="../../../GroupwareWorkbench/workspace"/>
    <property name="GW.this.location" value="../../GroupwareWorkbench/workspace"/>


    <property name="GW-Libs.this.location" value="${GW.this.location}/GW-Libs"/>
    <property name="GW-core.this.location" value="${GW.this.location}/GW-core"/>
    <property name="GW-collabElement.this.location" value="${GW.this.location}/GW-collabElement"/>
    <property name="GW-collablet.this.location" value="${GW.this.location}/GW-collablet"/>
    <property name="GW-commons-widgets.this.location" value="${GW.this.location}/GW-commons-widgets"/>

    <property name="GW-PhotoManager.location" value="GW-PhotoManager"/>
    <property name="GW-ExternalSiteManager.location" value="GW-ExternalSiteManager"/>

    <property name="debuglevel" value="source,lines,vars"/>
    <property name="target" value="1.6"/>
    <property name="source" value="1.6"/>
	
	<target description="Build all projects . Useful to propagate changes." name="build" >
		<!--<property name="build.compiler" value="false"/> -->
		<property name="build.sysclasspath" value="ignore"/>
		<ant antfile="${GW-core.this.location}/build.xml" inheritAll="true" target="clean"/>
		<ant antfile="${GW-core.this.location}/build.xml"  inheritAll="true" target="build-project"/>
		<ant antfile="${GW-collabElement.this.location}/build.xml"  inheritAll="true" target="clean"/>
		<ant antfile="${GW-collabElement.this.location}/build.xml"  inheritAll="true" target="build-project"/>
		<ant antfile="${GW-collablet.this.location}/build.xml"  inheritAll="true" target="clean"/>
		<ant antfile="${GW-collablet.this.location}/build.xml"  inheritAll="true" target="build-project"/>
		<ant antfile="${GW-commons-widgets.this.location}/build.xml"  inheritAll="true" target="clean"/>
		<ant antfile="${GW-commons-widgets.this.location}/build.xml"  inheritAll="true" target="build-project"/>
		
		<ant antfile="${GW-PhotoManager.location}/build.xml"  inheritAll="true" target="clean"/>
		<ant antfile="${GW-PhotoManager.location}/build.xml"  inheritAll="true" target="build-project" />
		<ant antfile="${GW-ExternalSiteManager.location}/build.xml"  inheritAll="true" target="clean"/>
		<ant antfile="${GW-ExternalSiteManager.location}/build.xml"  inheritAll="true" target="build-project" />
		<ant antfile="${GW-Application-Arquigrafia.location}/build.xml"  inheritAll="true" target="clean"/>
		<ant antfile="${GW-Application-Arquigrafia.location}/build.xml"  inheritAll="true" target="build-project" />


	</target>
</project>
